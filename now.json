  "version": 2,
  "scope": "gladly-team",
  "cleanUrls": true,
  "trailingSlash": true,
  "env": {
    "FIREBASE_CLIENT_EMAIL": "@environment-firebase-client-email",
    "FIREBASE_DATABASE_URL": "@environment-firebase-database-url",
    "FIREBASE_PRIVATE_KEY": "@environment-firebase-private-key",
    "FIREBASE_PROJECT_ID": "@environment-firebase-project-id",
  },
  "build": {
    "env": {
      "FIREBASE_PUBLIC_API_KEY": "@environment-firebase-public-api-key",
