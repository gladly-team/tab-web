{
  "name": "tab-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "go": " NODE_ENV=development vercel dev",
    "dev": "echo \"Use the 'yarn go' command to develop locally.\"",
    "build": "NODE_ENV=test yarn run test && NODE_ENV=production yarn run relay && next build",
    "start": "NODE_ENV=production next start",
    "test": "yarn run lint && yarn run relay && jest",
    "test:coverage": "yarn run test --coverage && codecov",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "yarn run format --write",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "relay": "relay-compiler --src ./src/ --artifactDirectory ./src/relay/__generated__ --exclude '**/__test__/**'  '**/__generated__/**' --schema ./src/schema/schema.graphql"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@sentry/browser": "^5.15.5",
    "@sentry/node": "^5.15.5",
    "@sentry/webpack-plugin": "^1.11.1",
    "@zeit/next-source-maps": "^0.0.4-canary.1",
    "clsx": "^1.1.0",
    "cookies": "^0.8.0",
    "firebase": "^7.16.1",
    "firebase-admin": "^9.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "next": "^9.4.4",
    "next-images": "^1.4.0",
    "next-offline": "^5.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebaseui": "^4.1.0",
    "react-relay": "^10.0.0",
    "relay-runtime": "^10.0.0",
    "tab-ads": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@testing-library/react-hooks": "^3.2.1",
    "babel-jest": "^26.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-relay": "^10.0.0",
    "codecov": "^3.6.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "graphql": "^15.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "mockdate": "^2.0.5",
    "prettier": "^2.0.5",
    "relay-compiler": "^10.0.0",
    "vercel": "^19.2.0"
  }
}
