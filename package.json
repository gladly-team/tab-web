{
  "name": "tab-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=test yarn run test && NODE_ENV=production yarn run relay && next build",
    "start": "NODE_ENV=production next start",
    "test": "yarn run lint && yarn run relay && jest",
    "test:coverage": "yarn run test --coverage && codecov",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "yarn run format --write",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "relay": "relay-compiler --src ./src/ --artifactDirectory ./src/relay/__generated__ --exclude '**/__test__/**'  '**/__generated__/**' --schema ./src/schema/schema.graphql",
    "graphql": "NODE_ENV=production graphql",
    "graphql:validate": "NODE_ENV=production graphql validate",
    "schema": "NODE_ENV=production graphql codegen"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "clsx": "^1.1.0",
    "cookies": "^0.8.0",
    "dotenv": "^8.2.0",
    "firebase": "^7.14.2",
    "firebase-admin": "^8.11.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "^9.3.6",
    "next-images": "^1.4.0",
    "next-offline": "^5.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebaseui": "^4.1.0",
    "react-relay": "^9.1.0",
    "relay-runtime": "^9.1.0",
    "tab-ads": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@graphql-codegen/schema-ast": "^1.13.5",
    "@test-graphql-cli/codegen": "4.0.1-beta.2",
    "@test-graphql-cli/validate": "4.0.1-beta.1",
    "@testing-library/react-hooks": "^3.2.1",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-relay": "^9.1.0",
    "codecov": "^3.6.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "graphql": "^15.0.0",
    "graphql-cli": "4.0.0-alpha-df0d2bc.481",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "relay-compiler": "^9.1.0"
  }
}
