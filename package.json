{
  "name": "tab-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=production yarn run relay && next build",
    "start": "NODE_ENV=production next start",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "yarn run format --write",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "relay": "relay-compiler --src ./ --artifactDirectory ./relay/__generated__ --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' --exclude '**/schema/**' --schema ./schema/schema.graphql",
    "graphql": "NODE_ENV=production graphql",
    "graphql:validate": "NODE_ENV=production graphql validate",
    "schema": "NODE_ENV=production graphql codegen"
  },
  "dependencies": {
    "cookie-session": "^1.3.3",
    "dotenv": "^8.2.0",
    "firebase": "^7.6.1",
    "firebase-admin": "^8.9.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "^9.1.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-firebaseui": "^4.0.0",
    "react-relay": "^7.1.0",
    "relay-runtime": "^7.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/schema-ast": "^1.9.1",
    "@test-graphql-cli/codegen": "4.0.0-alpha-df0d2bc.481",
    "@test-graphql-cli/validate": "4.0.0-alpha-df0d2bc.481",
    "babel-plugin-relay": "^7.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "graphql": "^14.5.8",
    "graphql-cli": "4.0.0-alpha-df0d2bc.481",
    "prettier": "^1.19.1",
    "relay-compiler": "^7.1.0"
  }
}
