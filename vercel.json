{
  "name": "tab-web",
  "version": 2,
  "scope": "gladly-team",
  "alias": ["dev-tab-web"],
  "cleanUrls": true,
  "trailingSlash": true,
  "rewrites": [
    { "source": "/v4/(.*)", "destination": "/newtab/$1" }
  ],
  "headers": [
    {
      "source": "/newtab/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/newtab/"
        }
      ]
    }
  ]
}
