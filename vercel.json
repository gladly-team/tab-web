{
  "name": "tab-web",
  "version": 2,
  "scope": "gladly-team",
  "alias": ["dev-tab-web"],
  "cleanUrls": true,
  "trailingSlash": true,
  "headers": [
    {
      "source": "/newtab/service-worker.js",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "no-cache"
        },
        {
          "key" : "Service-Worker-Allowed",
          "value" : "/newtab/"
        }
      ]
    }
  ]
}
